#include <iostream>
#include <fstream> 
#include <string>
using namespace std;
int main()
{
    int i,j,n,k;
    ofstream file;
    cin >> n >> k;
    file.open("krik dushi.txt");
    for (i = 0; i < n; i++) {
        for (j = 0; j < k; j++)
            file << "*";
        file << endl;
    }
    file.close();
    return 0;
}
